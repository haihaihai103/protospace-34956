<main class="main">
  <div class="inner">
    <% if user_signed_in? %>
      <%= link_to "こんにちは、#{current_user.name}さん", user_path(current_user.id) %>
      <%= render partial: "prototype", collection: @prototypes %>
      <% else %>
    <%# // ログインしているときは上記を表示する %>
    <div class="card__wrapper">
      <%= render partial: "prototype", collection: @prototypes %>
    </div>
    <% end %>
  </div>
</main>
